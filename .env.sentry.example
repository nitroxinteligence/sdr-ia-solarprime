# Sentry Configuration for SDR Agent
# Copy this to your .env file and fill in the values

# Sentry DSN (Data Source Name)
# Get this from your Sentry project settings
SENTRY_DSN=https://YOUR_SENTRY_KEY@o123456.ingest.sentry.io/PROJECT_ID

# Performance monitoring sample rate (0.0 to 1.0)
# 0.1 = 10% of transactions are sampled
SENTRY_TRACES_SAMPLE_RATE=0.1

# Profiling sample rate (0.0 to 1.0)
# 0.1 = 10% of transactions are profiled
SENTRY_PROFILES_SAMPLE_RATE=0.1

# Environment name (development, staging, production)
ENVIRONMENT=development